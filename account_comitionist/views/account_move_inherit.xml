<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Inherit Form View -->
    <record id="account_move_comisionista_inherit_view_form" model="ir.ui.view">
        <field name="name">account.move.inherit.view.form.inherit.module</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <!-- Modify an existing field -->
            <xpath expr="//field[@name='l10n_es_is_simplified']" position="after">
                <field name="comisionista"/>
            </xpath>
        </field>
    </record>
</odoo>